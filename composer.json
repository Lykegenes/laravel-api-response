{
    "name": "patricksamson/laravel-api-response",
    "description": "A Laravel wrapper for thephpleague's Fractal package",
    "keywords": [
        "lykegenes",
        "laravel-api-response",
        "fractal",
        "laravel",
        "api"
    ],
    "homepage": "https://github.com/patricksamson/laravel-api-response",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Samson",
            "email": "patricksamson236@gmail.com",
            "homepage": "https://github.com/patricksamson",
            "role": "Developer"
        }
    ],
    "require": {
	"php": "^7.3|^8.0",
        "illuminate/support": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/database": "^6.0",
        "league/fractal": "^0.19"
    },
    "require-dev": {
        "phpunit/phpunit" : "9.*",
        "mockery/mockery": "1.*",
        "orchestra/testbench": "~4.0",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Lykegenes\\ApiResponse\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/ApiResponseTestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
